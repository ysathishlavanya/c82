       <h1>
        Open console
       </h1> 
      <script>
        console.log("Hi i am console screen");

        var firstname = "Uma";
        console.log(firstname);

        var lastname = "Shankar";
         console.log(lastname);
 
         var fullname = firstname.concat (lastname);
         console.log(fullname);

         var length_of_full_name = fullname.length;
         console.log(length_of_full_name);

         var name_1= "Iam bala govind";
         console.log(name_1);

         var replace_name =name_1.replace("govind","murali");
         console.log(replace_name)

         var capital_letters = "JAYA";
         var lower_case = capital_letters.toLowerCase();
         console.log(lower_case);

         var square_root = Math.sqrt  ( 35);
         console.log(square_root);

         var maxium_number = Math.max (30, 60, 100);
         console.log(maxium_number);

         var minimum_number = Math.min (100, 200, 302);
         console.log(minimum_number);

         var power_number = Math.pow (2, 5);
         console.log(power_number);

         var power_number_2 = Math.pow (0,1);
         console.log(power_number_2);
      </script>


    
